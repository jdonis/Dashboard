@{
    Layout = null;
}
<html>
<head>
    <title>Información de los indicadores por país</title>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/DataTables/css/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/select.dataTables.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/buttons.dataTables.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/responsive.dataTables.css" rel="stylesheet" />
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
    <script src="~/scripts/jquery-3.3.1.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/scripts/DataTables/jquery.dataTables.js"></script>
    <script src="~/scripts/DataTables/dataTables.buttons.js"></script>
    <script src="~/scripts/DataTables/dataTables.select.js"></script>
    <script src="~/scripts/DataTables/dataTables.responsive.js"></script>
    <script src="~/scripts/DataTablesEditor/dataTables.altEditor.js"></script>
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <script src="~/scripts/jquery-ui.js"></script>

    <script src="~/Scripts/localconfig.js"></script>
    <script src="~/scripts/Catalog/FMP.js"></script>
</head>
<body>

    <h2>Ingreso de datos de indicadores por país</h2>

    <table id="DataTableCatalog" class="table, display"  width="100%" cellspacing="0">
        <thead>
            <tr>
                <th></th>
                <th>Country</th>
                <th>Code</th>
                <th>Year</th>

            </tr>
        </thead>
    </table>


    <div class="modal fade" id="altEditor-modal" tabindex="-1" role="dialog" dstyle="display: none; padding-left: 3px;">
        <div class="modal-dialog" style="max-width:1250px;">
            <div class="modal-content">
                <div class="modal-header">

                    <div class="col-sm-3 col-md-3 col-lg-3 text-right" style="padding-top:7px;"><label for="country">País:</label></div>
                    <select id="country_slc" name="country_slc" required data-validation="required" data-validation-if-checked-value="yes"></select>
                    <div class="col-sm-3 col-md-3 col-lg-3 text-right" style="padding-top:7px;"><label for="year_">Año:</label></div>
                    <select id="year_slc" name="year_slc" required data-validation="required" data-validation-if-checked-value="yes"></select>


                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                    <h4 class="modal-title">Ingreso de información indicadores</h4>
                </div>
                <div class="modal-body">
                    <div id="altEditor-form" name="altEditor-form" role="form">
                        <div id="tabs">
                            <ul>
                                <li><a href="#tab-WorkPlan">Work Plan</a></li>
                                <li><a href="#tab-EBPchecklist">EBP Checklist</a></li>
                                <li><a href="#tab-CPDchecklist">CPD Checklists</a></li>
                                <li><a href="#tab-ANEXOIIIchecklist">ANEXO III Checklist</a></li>
                                <li><a href="#tab-ICBchecklist">ICB Checklist</a></li>
                                <li><a href="#tab-TyLchecklist">T&L Checklist</a></li>
                            </ul>

                            <div id="tab-WorkPlan">

                                <div dclass="table-responsive">
                                    <table id="table-WorkPlan" class="table table-hover table-bordered table-striped">
                                        <thead>
                                            <tr class="font-weight-bold" id="table-WorkPlan-header">
                                                <td scope="col">Obj. Banco</td>
                                                <td scope="col">Main Business Functions</td>
                                                <td scope="col">Tipo</td>
                                                <td scope="col">
                                                    Metas <br />
                                                    <a href="#">Go to Manual de Indicadores</a>
                                                </td>
                                                <td scope="col">Frec.</td>
                                                <td scope="col">1Q <br />target</td>
                                                <td scope="col">Actual</td>
                                                <td scope="col">2Q <br />target</td>
                                                <td scope="col">Actual</td>
                                                <td scope="col">3Q <br />target</td>
                                                <td scope="col">Actual</td>
                                                <td scope="col">4Q <br />target</td>
                                                <td scope="col">Actual</td>
                                            </tr>
                                        </thead>
                                        <tbody id="table-tbody-WorkPlan"></tbody>


                                    </table>
                                    <div id="loading">
                                        <p><img src="~/Content/images/ajax-loader.gif" /> Please Wait</p>
                                    </div>

                                </div>
                            </div>

                            <div id="tab-EBPchecklist">
                                <div dclass="table-responsive">
                                    <table  id="table-EBPchecklist" class="table table-hover table-bordered table-striped">
                                        <thead>
                                            <tr class="font-weight-bold"  id="table-EBPchecklist-header">
                                                <td scope="col" id="indicator_EBPchecklist" colspan="2"></td>
                                                <td scope="col">1Q </td>
                                                <td scope="col">2Q </td>
                                                <td scope="col">3Q </td>
                                                <td scope="col">4Q </td>
                                                <td scope="col">Comentarios</td>
                                            </tr>
                                        </thead>
                                        <tbody id="table-tbody-EBPchecklist"></tbody>


                                    </table>

                                </div>
                            </div>
                            <div id="tab-CPDchecklist">
                                <div dclass="table-responsive">
                                    <table id="table-CPDchecklist" class="table table-hover table-bordered table-striped">
                                        <thead>
                                            <tr id="table-CPDchecklist-header">
                                                <td scope="col" id="indicator_CPDchecklist" colspan="2"></td>
                                                <td scope="col">1Q </td>
                                                <td scope="col">2Q </td>
                                                <td scope="col">3Q </td>
                                                <td scope="col">4Q </td>
                                                <td scope="col">Comentarios</td>
                                            </tr>
                                        </thead>
                                        <tbody id="table-tbody-CPDchecklist"></tbody>


                                    </table>
                                </div>
                            </div>
                            <div id="tab-ANEXOIIIchecklist">
                                <div dclass="table-responsive">
                                    <table  id="table-ANEXOIIIchecklist" class="table table-hover table-bordered table-striped">
                                        <thead>
                                            <tr id="table-ANEXOIIIchecklist-header">
                                                <td scope="col" id="indicator_ANEXOIIIchecklist" colspan="2"></td>
                                                <td scope="col">1Q </td>
                                                <td scope="col">2Q </td>
                                                <td scope="col">3Q </td>
                                                <td scope="col">4Q </td>
                                                <td scope="col">Comentarios</td>
                                            </tr>
                                        </thead>
                                        <tbody id="table-tbody-ANEXOIIIchecklist"></tbody>


                                    </table>
                                </div>
                            </div>
                            <div id="tab-ICBchecklist">
                                <div dclass="table-responsive">
                                    <table id="table-ICBchecklist" class="table table-hover table-bordered table-striped">
                                        <thead>
                                            <tr id="table-ICBchecklist-header">
                                                <td scope="col" id="indicator_ICBchecklist" colspan="2"></td>
                                                <td scope="col">1Q </td>
                                                <td scope="col">2Q </td>
                                                <td scope="col">3Q </td>
                                                <td scope="col">4Q </td>
                                                <td scope="col">Comentarios</td>
                                            </tr>
                                        </thead>
                                        <tbody id="table-tbody-ICBchecklist"></tbody>


                                    </table>
                                </div>
                            </div>
                            <div id="tab-TyLchecklist">
                                <div dclass="table-responsive">
                                    <table id="table-TyLchecklist" class="table table-hover table-bordered table-striped">
                                        <thead>
                                            <tr id="table-TyLchecklist-header">
                                                <td scope="col" id="indicator_TyLchecklist" colspan="2"></td>
                                                <td scope="col">1Q </td>
                                                <td scope="col">2Q </td>
                                                <td scope="col">3Q </td>
                                                <td scope="col">4Q </td>
                                                <td scope="col">Comentarios</td>
                                            </tr>
                                        </thead>
                                        <tbody id="table-tbody-TyLchecklist"></tbody>


                                    </table>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="modal-footer"><button type="button" data-content="remove" class="btn btn-default" data-dismiss="modal">Cerrar</button><button type="button" data-content="remove" class="btn btn-primary" id="editRowBtn">Guardar Cambios</button></div>
            </div>
        </div>
    </div>



</body>
</html>